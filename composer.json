{
    "name": "ag-develop/object-mirror",
    "description": "Library for mirroring objects over rabbitmq queues or any other medium using json serialized data",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AgDevelop\\ObjectMirror\\": "src/",
            "AgDevelop\\ObjectMirror\\Tests\\": "test/"
        }
    },
    "repositories" : [
        {
            "type" : "vcs",
            "url" : "https://github.com/ag-develop/interfaces"
        }
    ],
    "require": {
        "spatie/data-transfer-object": "^3.8",
        "ag-develop/interfaces": "dev-feature/basic-interfaces-defined"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpunit/phpunit": "^9.5"
    }
}
